<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse" name="PizzashackByIdProxy" transports="http https" startOnLoad="true" trace="disable">
    <target>
        <inSequence>
            <filter xpath="//pizzashackId">
                <then>
                    <property name="REST_URL_POSTFIX" expression="//pizzashackId" scope="axis2" type="STRING"/>
                    <property name="HTTP_METHOD" value="GET" scope="axis2" type="STRING"/>
                </then>
                <else/>
            </filter>
            <spring:spring xmlns:spring="http://ws.apache.org/ns/synapse" bean="pizzashackSearchByIdSendMediator" key="conf:custom/pizzashack-wso2-spring/pizzashack-springmediator.xml" description="spring:send"/>
            <log level="full" description="log:beforelog"/>
            <send>
                <endpoint key="PizzashackByIdEP"/>
            </send>
        </inSequence>
        <outSequence>
            <log level="full" description="log:receive"/>
            <spring:spring xmlns:spring="http://ws.apache.org/ns/synapse" bean="pizzashackSearchByIdRecieveMediator" key="conf:custom/pizzashack-wso2-spring/pizzashack-springmediator.xml" description="spring:receive"/>
            <send/>
        </outSequence>
        <faultSequence/>
    </target>
</proxy>
